programa {
  funcao inicio() {
    real numero1, numero2, resultado

    escreva("Forneça o primeiro número: ")
    leia(numero1)
    escreva("Forneça o segundo número: ")
    leia(numero2)

    resultado = numero1 - numero2

    escreva(numero1, " - ", numero2, " = ", resultado)
    escreva(resultado)
  }
}
